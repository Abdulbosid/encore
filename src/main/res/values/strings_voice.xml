<?xml version="1.0" encoding="utf-8"?>
<resources>
    <string-array name="voice_play_words">
        <item>play</item>           <!-- Play Coldplay -->
        <item>listen to</item>      <!-- Listen to Californication -->
        <item>start</item>          <!-- Start Starred -->
    </string-array>

    <string-array name="voice_play_source_words">
        <item>from</item>           <!-- Play Justin Bieber from Spotify -->
        <item>on</item>             <!-- Start Starred on Spotify -->
    </string-array>

    <string-array name="voice_pause_words">
        <item>pause</item>          <!-- Pause -->
    </string-array>

    <string-array name="voice_jump_words">
        <item>jump to track</item>          <!-- Jump to track 10 -->
        <item>go to track</item>            <!-- Go to track 12 -->
    </string-array>

    <string-array name="voice_next_words">
        <item>next</item>           <!-- Next -->
