    </string-array>

    <string-array name="voice_time_minutes">
        <item>minute</item>
        <item>minutes</item>                <!-- Skip 2 minutes -->
    </string-array>

    <string-array name="voice_google_words">
        <item>google</item>                     <!-- Google what's the weather -->
        <item>search the web for</item>         <!-- Search the web for australopithecus -->
