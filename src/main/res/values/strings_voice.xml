    <string-array name="voice_previous_words">
        <item>rewind</item>                 <!-- Rewind -->
        <item>previous</item>               <!-- Previous -->
    </string-array>

    <string-array name="voice_time_seconds">
        <item>seconds</item>                <!-- Rewind 10 seconds -->
